    "type": "library",
    "require": {
        "php": ">=8.2",
        "webonyx/graphql-php": "^15.5",
        "filecage/graphql-annotations": "dev-main#0e69ab7"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Filecage\\GraphQL\\Factory\\": "src/"
        }
    },
    "authors": [
        {
            "name": "David",
            "email": "filecage@users.noreply.github.com"
        }
    ],
    "require-dev": {
